<launch>
    <arg name="robot_id" default="mkmrX"/>
    <arg name="is_sim" default="false"/>
    <arg name="gazebo_world_name" default="mkmr_test_world"/>


    <param name="$(arg robot_id)/robot_description" command="$(find xacro)/xacro '$(find mkmr_description)/urdf/mkmr.xacro'
                                             robot_id:=$(arg robot_id)
                                             is_sim:=$(arg is_sim)"/>
                                        

</launch>
